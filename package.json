{
  "name": "sftp-to-papertrail",
  "version": "0.1.0",
  "description": "A Node module that retrieves one or more log files via SFTP, and logs new entries to Papertrail. Deploys to AWS Lambda and uses S3 for maintaining state.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdmalone/sftp-to-papertrail.git"
  },
  "main": "index.js",
  "author": "Tim Malone <tdmalone@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tdmalone/sftp-to-papertrail/issues"
  },
  "homepage": "https://github.com/tdmalone/sftp-to-papertrail#readme",
  "scripts": {
    "execute": "node -e \"require( '.' ).handler( {}, {}, ( err ) => { if ( err ) throw new Error( err ) } )\"",
    "lint": "eslint --ignore-pattern '/coverage/' --ignore-pattern '/docs/' --ignore-pattern '!.eslintrc.js' \"**/*.js\"",
    "lint:docs": "documentation lint *.js",
    "fix": "yarn lint --fix",
    "docs": "documentation build *.js --output docs --format html --sort-order alpha",
    "test": "jest --verbose --coverage"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.js"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.170.0",
    "ssh2-sftp-client": "^2.0.1",
    "winston": "^2.4.0",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "@chromatix/eslint-config-chromatix": "^0.0.14",
    "jest": "^21.2.1",
    "jest-tobetype": "^1.1.0"
  }
}
